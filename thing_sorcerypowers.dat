    <tag group="ValSorc" tag="naAvalon"/>
    <tag group="GlaMinor" tag="Luck"/>
    </thing>

  <thing
    id="pGlaGreLuc"
    name="Greater Luck"
    compset="PowerMin"
    isunique="no"
    description="Activate this Glamour to add your Rank in this Glamour to the value of any die. For example, if you have 5 Ranks in this Glamour, you could add 5 points to any die you rolled. If the value of a die reaches 10 or higher, that die Explodes."
    summary="">
    <tag group="Sorcery" tag="Minor"/>
    <tag group="Sorcery" tag="PwrRanked"/>
    <tag group="ValSorc" tag="naAvalon"/>
    <tag group="GlaMinor" tag="Luck"/>
    </thing>

  <thing
    id="pGlaHeroic"
    name="Heroic"
    compset="PowerMin"
    isunique="no"
    description="Activate this Glamour after a Risk using your Minor Trait. You may re-roll a number of dice equal to your Rank in this Glamour. You must keep the new value on any dice that you re-roll, even if they are lower."
    summary="">
    <tag group="Sorcery" tag="Minor"/>
    <tag group="Sorcery" tag="PwrRanked"/>
    <tag group="ValSorc" tag="naAvalon"/>
    <tag group="GlaMinor" tag="Luck"/>
    </thing>

  <thing
    id="pGlaMythic"
    name="Mythic"
    compset="PowerMaj"
    isunique="no"
    description="Activate this Glamour after a Risk using your Major Trait. You may re-roll a number of dice equal to your Rank in this Glamour. You may choose which value to keep between the old roll and the new roll."
    summary="">
    <tag group="Sorcery" tag="Major"/>
    <tag group="Sorcery" tag="PwrRanked"/>
    <tag group="ValSorc" tag="naAvalon"/>
    <tag group="GlaMajor" tag="Luck"/>
    </thing>

  <thing
    id="pGlaMadLuc"
    name="Mad Luck"
    compset="PowerMaj"
    isunique="no"
    description="Activate this Glamour to roll a number of dice equal to your Rank in this Glamour. You may pass these dice out to other Heroes who may keep that die for all rolls for the rest of the Scene. For example, if you choose to give an 8 to a duelist, for the rest of the Scene that Hero always adds that die to every Risk."
    summary="">
    <tag group="Sorcery" tag="PwrRanked"/>
    <tag group="Sorcery" tag="Major"/>
    <tag group="ValSorc" tag="naAvalon"/>
    <tag group="GlaMajor" tag="Luck"/>
    </thing>

  <thing
    id="pGlaLegend"
    name="Legend"
    compset="PowerMaj"
    isunique="no"
    description="Activate this Glamour to add your Ranks in this Glamour to your Major Trait for the rest of the Scene."
    summary="">
    <tag group="Sorcery" tag="PwrRanked"/>
    <tag group="Sorcery" tag="Major"/>
    <tag group="ValSorc" tag="naAvalon"/>
    <tag group="GlaMajor" tag="Luck"/>
    </thing>
  
  <thing
    id="pGlaRedBru"
    name="Reduce the Brute"
    compset="PowerMin"
    isunique="no"
    description="Activate this Glamour to eliminate any Brute Squads in a Scene with a Strength rating equal to or less than the Ranks you have in this Glamour."
    summary="">
    <tag group="Sorcery" tag="PwrRanked"/>
    <tag group="Sorcery" tag="Minor"/>
    <tag group="ValSorc" tag="naAvalon"/>
    <tag group="GlaMinor" tag="attrBra"/>
    </thing> 
  
  <thing
    id="pGlaStrYou"
    name="Stronger Than You"
    compset="PowerMin"
    isunique="no"
    description="Activate this Glamour to add a number of dice to any Brawn Risk equal to your Ranks in this Glamour, plus one."
    summary="">
    <tag group="Sorcery" tag="PwrRanked"/>
    <tag group="Sorcery" tag="Minor"/>
    <tag group="ValSorc" tag="naAvalon"/>
    <tag group="GlaMinor" tag="attrBra"/>
    </thing> 
  
  <thing
    id="pGlaRedVil"
    name="Reduce the Villain"
    compset="PowerMin"
    isunique="no"
    description="Activate this Glamour to temporarily reduce a Villain's Strength by the number of Ranks you have in this Glamour. The Villain's Strength returns at the end of the Episode."
    summary="">
    <tag group="Sorcery" tag="PwrRanked"/>
    <tag group="Sorcery" tag="Minor"/>
    <tag group="ValSorc" tag="naAvalon"/>
    <tag group="GlaMinor" tag="attrBra"/>
    </thing> 
  
  <thing
    id="pGlaStrTIs"
    name="Strongest There Is"
    compset="PowerMaj"
    isunique="no"
